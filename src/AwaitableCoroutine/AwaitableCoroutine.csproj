<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net5.0;net6.0</TargetFrameworks>
    <PackageOutputPath>../../output</PackageOutputPath>

    <PackageId>AwaitableCoroutine</PackageId>
    <Version>1.7.0</Version>
    <Description>AwaitableCoroutine is a library for C# that provides a coroutine that allows the use of async/await syntax.</Description>
    <PackageProjectUrl>https://github.com/wraikny/AwaitableCoroutine</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright (c) 2021 - 2022 wraikny</Copyright>
    <RepositoryType>git</RepositoryType>
    <PackageTags>coroutine,awaitable,asyncawait</PackageTags>
  </PropertyGroup>

</Project>
