<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net5.0;net6.0</TargetFrameworks>
    <PackageOutputPath>../../output</PackageOutputPath>
    <PlatformTarget>x64</PlatformTarget>

    <PackageId>AwaitableCoroutine.Altseed2</PackageId>
    <Version>1.6.0</Version>
    <Description>AwaitableCoroutine extension for Altseed2</Description>
    <PackageProjectUrl>https://github.com/wraikny/AwaitableCoroutine</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright (c) 2021 - 2022 wraikny</Copyright>
    <RepositoryType>git</RepositoryType>
    <PackageTags>coroutine,awaitable,asyncawait,altseed2</PackageTags>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Altseed2" Version="2.1.*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AwaitableCoroutine\AwaitableCoroutine.csproj" />
  </ItemGroup>

</Project>
